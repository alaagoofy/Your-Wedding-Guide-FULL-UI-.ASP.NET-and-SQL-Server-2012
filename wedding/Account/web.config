<?xml version="1.0"?>
<configuration>

  <location path="~/Register.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
        <deny users="*" />
      </authorization>
    </system.web>
  </location>

  <system.web>
    <authorization>
      <deny users="?" />
      <allow roles="Admin" />
      <deny users="*" />
    </authorization>
  </system.web>

</configuration>
